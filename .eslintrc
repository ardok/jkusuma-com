{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "indent": [2, 2, { "SwitchCase": 1 }],
    "jsx-quotes": [2, "prefer-double"],
    "max-len": 0,
    "object-curly-spacing": [2, "never"],
    "no-param-reassign": 0,
    "react/jsx-curly-spacing": [2, "never", { "allowMultiline": true }]
  }
}
